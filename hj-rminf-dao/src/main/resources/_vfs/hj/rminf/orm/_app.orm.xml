<?xml version="1.0" encoding="UTF-8" ?>
<orm ext:mavenArtifactId="hj-rminf" ext:entityPackageName="com.hj.rminf.dao.entity" ext:registerShortName="true"
     ext:mavenGroupId="com.hj" ext:basePackageName="com.hj.rminf" ext:appName="hj-rminf"
     ext:platformVersion="2.0.0-SNAPSHOT" ext:dialect="mysql,oracle,postgresql" ext:mavenVersion="1.0.0"
     x:schema="/nop/schema/orm/orm.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:i18n-en="i18n-en"
     xmlns:ref-i18n-en="ref-i18n-en" xmlns:ext="ext" xmlns:orm-gen="orm-gen" xmlns:xpl="xpl" xmlns:ui="ui">

    <x:post-extends x:override="replace">
        <orm-gen:DefaultPostExtends xpl:lib="/nop/orm/xlib/orm-gen.xlib"/>
    </x:post-extends>

    <domains>
        <domain name="createTime" stdSqlType="DATETIME"/>
        <domain name="createdBy" precision="50" stdSqlType="VARCHAR"/>
        <domain name="updateTime" stdSqlType="DATETIME"/>
        <domain name="updatedBy" precision="50" stdSqlType="VARCHAR"/>
        <domain name="delFlag" stdSqlType="BOOLEAN"/>
        <domain name="image" precision="100" stdSqlType="VARCHAR"/>
    </domains>

    <entities>
        <entity className="com.hj.rminf.dao.entity.RminfJgVehicleinfo" createTimeProp="addTime" deleteFlagProp="deleted"
                displayName="集光过车（超员）" name="com.hj.rminf.dao.entity.RminfJgVehicleinfo" registerShortName="true"
                tableName="rminf_jg_vehicleinfo" updateTimeProp="updateTime" useLogicalDelete="true">
            <columns>
                <column code="ID" displayName="Id" mandatory="true" name="id_" primary="true" propId="1"
                        stdDataType="int" stdSqlType="INTEGER" tagSet="seq" ui:show="R"/>
                <column code="KKBH" displayName="卡口编号" mandatory="true" name="kkbh" precision="32" propId="2"
                        stdDataType="string" stdSqlType="VARCHAR"/>
                <column code="HPHM" displayName="号牌号码" mandatory="true" name="hphm" precision="32" propId="3"
                        stdDataType="string" stdSqlType="VARCHAR"/>
                <column code="TP1" displayName="通行图片1" domain="image" name="tp1" precision="255" propId="4"
                        stdDataType="string" stdSqlType="VARCHAR"/>
                <column code="TP2" displayName="通行图片2" domain="image" name="tp2" precision="255" propId="5"
                        stdDataType="string" stdSqlType="VARCHAR"/>
                <column code="TP3" displayName="通行图片3" domain="image" name="tp3" precision="255" propId="6"
                        stdDataType="string" stdSqlType="VARCHAR"/>
                <column code="ADD_TIME" displayName="创建时间" domain="createTime" name="addTime" propId="7"
                        stdDataType="datetime" stdSqlType="DATETIME" ui:show="X"/>
                <column code="UPDATE_TIME" displayName="更新时间" domain="updateTime" name="updateTime" propId="8"
                        stdDataType="datetime" stdSqlType="DATETIME" ui:show="X"/>
                <column code="DELETED" displayName="逻辑删除" domain="delFlag" name="deleted" propId="9"
                        stdDataType="boolean" stdSqlType="BOOLEAN" ui:show="X"/>
            </columns>
        </entity>
    </entities>
</orm>