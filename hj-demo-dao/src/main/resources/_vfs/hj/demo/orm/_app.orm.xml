<?xml version="1.0" encoding="UTF-8" ?>
<orm ext:mavenArtifactId="hj-demo" ext:entityPackageName="com.hj.demo.dao.entity" ext:registerShortName="true"
     ext:mavenGroupId="com.hj" ext:basePackageName="com.hj.demo" ext:appName="hj-demo"
     ext:platformVersion="2.0.0-SNAPSHOT" ext:dialect="mysql,oracle,postgresql" ext:mavenVersion="1.0.0"
     x:schema="/nop/schema/orm/orm.xdef" xmlns:x="/nop/schema/xdsl.xdef" xmlns:i18n-en="i18n-en"
     xmlns:ref-i18n-en="ref-i18n-en" xmlns:ext="ext" xmlns:orm-gen="orm-gen" xmlns:xpl="xpl" xmlns:ui="ui">

    <x:post-extends x:override="replace">
        <orm-gen:DefaultPostExtends xpl:lib="/nop/orm/xlib/orm-gen.xlib"/>
    </x:post-extends>

    <domains>
        <domain name="createTime" stdSqlType="DATETIME"/>
        <domain name="createdBy" precision="50" stdSqlType="VARCHAR"/>
        <domain name="updateTime" stdSqlType="DATETIME"/>
        <domain name="updatedBy" precision="50" stdSqlType="VARCHAR"/>
        <domain name="delFlag" stdSqlType="BOOLEAN"/>
    </domains>

    <entities>
        <entity className="com.hj.demo.dao.entity.NopDemo" createTimeProp="addTime" deleteFlagProp="deleted"
                displayName="Nopdemo" name="com.hj.demo.dao.entity.NopDemo" registerShortName="true"
                tableName="nop_demo" updateTimeProp="updateTime" useLogicalDelete="true">
            <columns>
                <column code="ID" displayName="Id" mandatory="true" name="id_" primary="true" propId="1"
                        stdDataType="int" stdSqlType="INTEGER" tagSet="seq" ui:show="R"/>
                <column code="NAME" displayName="名称" mandatory="true" name="name" precision="255" propId="2"
                        stdDataType="string" stdSqlType="VARCHAR"/>
                <column code="ENABLED" defaultValue="false" displayName="是否启用" mandatory="true" name="enabled"
                        propId="3" stdDataType="boolean" stdSqlType="BOOLEAN"/>
                <column code="ADD_TIME" displayName="创建时间" domain="createTime" name="addTime" propId="4"
                        stdDataType="datetime" stdSqlType="DATETIME" ui:show="X"/>
                <column code="UPDATE_TIME" displayName="更新时间" domain="updateTime" name="updateTime" propId="5"
                        stdDataType="datetime" stdSqlType="DATETIME" ui:show="X"/>
                <column code="DELETED" displayName="逻辑删除" domain="delFlag" name="deleted" propId="6"
                        stdDataType="boolean" stdSqlType="BOOLEAN" ui:show="X"/>
            </columns>
        </entity>
    </entities>
</orm>